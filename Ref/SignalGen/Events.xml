  SequenceFileLoader
  Events

======================================================================-->

<events>
  <event
    id="0"
    name="SignalGen_SettingsChanged"
    severity="ACTIVITY_LO"
    format_string="Set Frequency(Hz) %d, Amplitude %f, Phase %f, Signal Type %s">
      <comment>Signal Generator Settings Changed</comment>
      <args>
        <arg 
          type="U32" 
          name="Frequency">
        </arg>
        <arg
          type="F32"
          name="Amplitude">
        </arg>
        <arg
          type="F32"
          name="Phase">
        </arg>
        <arg
          type="Ref::SignalType"
          name="SignalType">
        </arg>
      </args>
  </event>
</events>
