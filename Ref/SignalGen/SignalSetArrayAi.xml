<?xml version="1.0" encoding="UTF-8"?>
<!--
SignalArray:

An example signal array that downlinks the last 4 samples generated, as an array type.
-->
<array name="SignalSet" namespace="Ref">
    <type>F32</type>
    <size>4</size>
    <format>%f</format>

    <!-- In XML defaults are required -->
    <default>
        <value>0.0f</value>
        <value>0.0f</value>
        <value>0.0f</value>
        <value>0.0f</value>
    </default>
</array>
