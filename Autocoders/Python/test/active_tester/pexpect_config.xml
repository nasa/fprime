<test_module path="/Autocoders/Python/test/active_tester">
	<test_case name="Sync_InputPortF32">
		<p_group>
			<p_expect>Enter g to go or q to quit:</p_expect>
			<p_send>g</p_send>
		</p_group>
		<p_group>
			<p_expect>Enter u for unsigned-int and f for float:</p_expect>
			<p_send>f</p_send>
		</p_group>
		<p_group>
			<p_expect>Enter a float to send to CompA:</p_expect>
			<p_send>3.140000</p_send>
		</p_group>
		<p_group>
			<p_expect>Comp_A Received Input: 3.140000</p_expect>
			<p_send>q</p_send>
		</p_group>
	</test_case>
	<test_case name="Async_InputPortU32">
		<p_group>
			<p_expect>Enter g to go or q to quit:</p_expect>
			<p_send>g</p_send>
		</p_group>
		<p_group>
			<p_expect>Enter u for unsigned-int and f for float:</p_expect>
			<p_send>u</p_send>
		</p_group>
		<p_group>
			<p_expect>Enter an unsigned-integer to send to CompA:</p_expect>
			<p_send>10</p_send>
		</p_group>
		<p_group>
			<p_expect>Comp_A Received Input: 10</p_expect>
			<p_send>q</p_send>
		</p_group>
	</test_case>
</test_module>
