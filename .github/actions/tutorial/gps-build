#!/bin/bash

# Exit when any command fails
set -e
set -x

# Clone community repo for GpsApp
git clone --recurse-submodules https://github.com/fprime-community/gps-tutorial.git

# Update fprime directory w/ latest release
cd gps-tutorial/fprime
git fetch

# Build GpsApp
cd GpsApp
fprime-util generate
fprime-util build